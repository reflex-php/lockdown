{
    "name": "reflex/lockdown",
    "description": "PHP 5.4 ACL system",
    "keywords": ["auth", "laravel", "acl", "lockdown"],
    "license": "MIT",
    "homepage": "https://github.com/reflex-php/lockdown",
    "support": {
        "issues": "https://github.com/reflex-php/lockdown/issues",
        "source": "https://github.com/reflex-php/lockdown"
    },
    "authors": [
        {
            "name": "Mike Shellard",
            "email": "mike.shellard@outlook.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "illuminate/cookie": "~5.0",
        "illuminate/cache": "~5.0",
        "illuminate/database": "~5.0",
        "illuminate/http": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit":  "~4.0",
        "mockery/mockery":  "~0.9"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4":    {
            "Reflex\\Lockdown\\": "src/"
        },
        "files":    [
            "src/helpers.php",
            "src/exceptions.php"
        ]
    },
    "minimum-stability": "dev"
}
